        "/api/v1/characters": {
            "get": {
                "summary": "characters",
                "description": "Retrieve characters with ability to sort and filter",
                "operationId": "characters",
                "parameters": [
                    {
                        "name": "sort_by",
                        "in": "query",
                        "schema": {
                            "type": "string",
                            "example": "name"
                        },
                        "description": "name or height"
                    },
                    {
                        "name": "order_by",
                        "in": "query",
                        "schema": {
                            "type": "string",
                            "example": "asc"
                        },
                        "description": "asc or desc"
                    },
                    {
                        "name": "gender",
                        "in": "query",
                        "schema": {
                            "type": "string",
                            "example": "male"
                        },
                        "description": "filter by gender"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "characters",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "body": {
                                            "type": "object",
                                            "properties": {
                                                "charactersObj": {
                                                    "type": "array",
                                                    "items": {
