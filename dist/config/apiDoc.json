        "/api/v1/movies/{id}": {
            "get": {
                "summary": "singleMovie",
                "description": "Retrieve a movie and it's comments by movie id",
                "operationId": "singlemovie",
                "responses": {
                    "200": {
                        "description": "singleMovie_example",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "body": {
                                            "type": "object",
                                            "properties": {
                                                "comments": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "object",
                                                        "properties": {
                                                            "createdAt": {
                                                                "type": "string",
                                                                "example": "2023-04-01T16:48:38.026Z"
                                                            },
                                                            "filmId": {
                                                                "type": "number",
                                                                "example": 1
                                                            },
                                                            "id": {
                                                                "type": "string",
                                                                "example": "clfy7jxdn0000fqi0fjt1vfr1"
                                                            },
                                                            "ipAddress": {
                                                                "type": "string",
                                                                "example": "::ffff:127.0.0.1"
                                                            },
                                                            "text": {
                                                                "type": "string",
                                                                "example": "lovely movie. very exciting"
                                                            }
                                                        }
                                                    },
                                                    "example": [
                                                        {
                                                            "createdAt": "2023-04-01T16:48:38.026Z",
                                                            "filmId": 1,
                                                            "id": "clfy7jxdn0000fqi0fjt1vfr1",
                                                            "ipAddress": "::ffff:127.0.0.1",
                                                            "text": "lovely movie. very exciting"
                                                        }
                                                    ]
                                                },
                                                "movie_title": {
                                                    "type": "string",
                                                    "example": "A New Hope"
                                                },
                                                "opening_crawl": {
                                                    "type": "string",
                                                    "example": "It is a period of civil war.\r\nRebel spaceships, striking\r\nfrom a hidden base, have won\r\ntheir first victory against\r\nthe evil Galactic Empire.\r\n\r\nDuring the battle, Rebel\r\nspies managed to steal secret\r\nplans to the Empire's\r\nultimate weapon, the DEATH\r\nSTAR, an armored space\r\nstation with enough power\r\nto destroy an entire planet.\r\n\r\nPursued by the Empire's\r\nsinister agents, Princess\r\nLeia races home aboard her\r\nstarship, custodian of the\r\nstolen plans that can save her\r\npeople and restore\r\nfreedom to the galaxy...."
                                                },
                                                "release_date": {
                                                    "type": "string",
                                                    "example": "1977-05-25"
                                                }
                                            }
                                        },
                                        "status": {
                                            "type": "string",
                                            "example": "success"
                                        }
                                    }
                                },
                                "examples": {
                                    "singleMovie_example": {
                                        "value": {
                                            "body": {
                                                "comments": [
                                                    {
                                                        "createdAt": "2023-04-01T16:48:38.026Z",
                                                        "filmId": 1,
                                                        "id": "clfy7jxdn0000fqi0fjt1vfr1",
                                                        "ipAddress": "::ffff:127.0.0.1",
                                                        "text": "lovely movie. very exciting"
                                                    }
                                                ],
                                                "movie_title": "A New Hope",
                                                "opening_crawl": "It is a period of civil war.\r\nRebel spaceships, striking\r\nfrom a hidden base, have won\r\ntheir first victory against\r\nthe evil Galactic Empire.\r\n\r\nDuring the battle, Rebel\r\nspies managed to steal secret\r\nplans to the Empire's\r\nultimate weapon, the DEATH\r\nSTAR, an armored space\r\nstation with enough power\r\nto destroy an entire planet.\r\n\r\nPursued by the Empire's\r\nsinister agents, Princess\r\nLeia races home aboard her\r\nstarship, custodian of the\r\nstolen plans that can save her\r\npeople and restore\r\nfreedom to the galaxy....",
                                                "release_date": "1977-05-25"
                                            },
                                            "status": "success"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "parameters": [
                {
                    "name": "id",
                    "in": "path",
                    "required": true,
                    "schema": {
                        "type": "integer",
                        "example": "1"
                    }
                }
            ]
        },
